{
  "name": "elgato-stream-deck",
  "version": "2.1.1",
  "description": "An npm module for interfacing with the Elgato Stream Deck",
  "main": "index.js",
  "typings": "build/exports.d.ts",
  "dependencies": {
    "node-hid": "^0.7.3",
    "sharp": "^0.21.0"
  },
  "devDependencies": {
    "@types/node": "^10.11.7",
    "@types/node-hid": "^0.7.0",
    "@types/sharp": "^0.17.10",
    "ava": "^0.23.0",
    "coveralls": "^3.0.2",
    "eslint": "^4.19.1",
    "eslint-config-xo": "^0.19.0",
    "eslint-plugin-ava": "^4.5.1",
    "mockery": "^2.1.0",
    "nyc": "^11.9.0",
    "pureimage": "0.1.3",
    "sinon": "^4.5.0",
    "standard-version": "^4.4.0",
    "stream-buffers": "^3.0.2",
    "weallbehave": "^1.2.0",
    "weallcontribute": "^1.0.8"
  },
  "scripts": {
    "prerelease": "npm t",
    "release": "standard-version",
    "postrelease": "npm publish && git push --follow-tags",
    "pretest": "npm run static",
    "test": "nyc ava",
    "report": "nyc report --reporter=html",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "static": "eslint .",
    "update-coc": "weallbehave -o . && git add CODE_OF_CONDUCT.md && git commit -m 'docs(coc): updated CODE_OF_CONDUCT.md'",
    "update-contrib": "weallcontribute -o . && git add CONTRIBUTING.md && git commit -m 'docs(contributing): updated CONTRIBUTING.md'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lange/node-elgato-stream-deck.git"
  },
  "keywords": [
    "elgato",
    "stream",
    "deck",
    "streamdeck",
    "hid",
    "usb",
    "hardware",
    "interface",
    "controller"
  ],
  "author": {
    "name": "Alex Van Camp",
    "email": "email@alexvan.camp",
    "url": "https://alexvan.camp/",
    "twitter": "vancamp"
  },
  "contributors": [
    {
      "name": "Richard Fox",
      "twitter": "ProbablePrime"
    },
    {
      "name": "Chris Hanel",
      "twitter": "chrishanel",
      "email": "chrishanel@gmail.com",
      "url": "http://www.chrishanel.com/"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Lange/node-elgato-stream-deck/issues"
  },
  "homepage": "https://github.com/Lange/node-elgato-stream-deck#readme",
  "eslintConfig": {
    "extends": "xo/esnext"
  },
  "files": [
    "*.js",
    "typings.d.ts",
    "lib"
  ],
  "engines": {
    "node": ">=7.6"
  }
}
